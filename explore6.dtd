<?xml encoding="UTF-8"?>

<!ELEMENT liste-pages (ns)+>
<!ATTLIST liste-pages
  xmlns CDATA #FIXED ''
  nb CDATA #REQUIRED>

<!ELEMENT ns (patchs,tailles,robots,users)>
<!ATTLIST ns
  xmlns CDATA #FIXED ''
  nb CDATA #REQUIRED
  nom CDATA #REQUIRED>

<!ELEMENT tailles (max+,random+)>
<!ATTLIST tailles
  xmlns CDATA #FIXED ''
  avg_patch CDATA #REQUIRED
  max CDATA #REQUIRED
